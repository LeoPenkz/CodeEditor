{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "kebab-case"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "no-input-rename": false
  }
}
